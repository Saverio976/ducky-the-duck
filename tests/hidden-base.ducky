REM TITLE Hidden command
REM AUTHOR Saverio976
REM DESCRIPTION echo something
$_JITTER_ENABLED = TRUE
STRINGLN [[ "$SHELL" == *"zsh"* ]] && zsh
STRINGLN [[ "$SHELL" == *"bash"* ]] && bash
STRINGLN cd
STRINGLN [[ "$SHELL" == *"zsh"* ]] && unset HISTFILE
STRINGLN [[ "$SHELL" == *"bash"* ]] && set +o history
STRINGLN echo hidden command
STRINGLN exit
ATTACKMODE OFF
